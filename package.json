{
  "name": "neptunbrigad",
  "version": "1.0.1",
  "description": "Neptun BrigÃ¡d static website",
  "author": "Roland Toth",
  "license": "MIT",
  "scripts": {
    "imgResize": "node scripts/imgResize",
    "imgMin": "node scripts/imgMin",
    "build:css": "node-sass --include-path src/scss/includes src/scss/style.scss | postcss --use autoprefixer -b \"last 2 versions\" --use cssnano --no-map > input/includes/style.css",
    "lint": "jshint src/js/main.js",
    "build:js": "uglifyjs src/js/main.js > assets/scripts/main.min.js",
    "build": "npm run build:css && npm run build:js",
    "prebuild:js": "npm run lint",
    "watch:css": "onchange \"src/scss/*.scss\" -- npm run build:css",
    "watch:js": "onchange \"src/js/*.js\" -- npm run build:js",
    "watch": "parallelshell \"npm run watch:css\" \"npm run watch:js\""
  },
  "dependencies": {
    "@11ty/eleventy": "^0.7.1",
    "autoprefixer": "^9.5.0",
    "cssnano": "^4.1.10",
    "del": "^3.0.0",
    "get-video-id": "^3.1.0",
    "glob": "^7.1.3",
    "html-minifier": "^3.5.20",
    "image-downloader": "^3.4.1",
    "image-size": "^0.6.3",
    "imagemin": "^6.0.0",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^6.0.0",
    "jshint": "^2.10.2",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.11.0",
    "onchange": "^5.2.0",
    "parallelshell": "^3.0.2",
    "path": "^0.12.7",
    "postcss": "^7.0.14",
    "postcss-cli": "^6.1.2",
    "sharp": "^0.20.7",
    "uglify-js": "^3.5.2",
    "video-thumbnail-url": "^1.0.1"
  }
}
